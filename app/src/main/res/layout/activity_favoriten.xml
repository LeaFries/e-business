        android:layout_height="wrap_content"
        android:height="20dp"
        android:checkable="false"
        android:clickable="true"
        android:contentDescription="@string/favoriten_headline"
        app:icon="@drawable/heart_icon_foreground"
        app:iconPadding="0dp"
        app:iconSize="60dp"
        app:iconTint="#F44336"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
